# Homologation Environment Configuration
ENVIRONMENT=homologation

# Database - RDS PostgreSQL (t3.micro)
DATABASE_URL=postgresql://ducktickets:CHANGE_PASSWORD@ducktickets-hml.cluster-xxxxx.us-east-1.rds.amazonaws.com:5432/ducktickets

# Security
SECRET_KEY=GENERATE_STRONG_SECRET_KEY_32_CHARS_MIN
ALLOWED_ORIGINS=https://ducktickets-hml.us-east-1.elasticbeanstalk.com

# AWS Services
AWS_REGION=us-east-1
S3_BUCKET=ducktickets-hml-storage
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/ACCOUNT_ID/ducktickets-hml-queue

# External Services (sandbox/test)
MERCADO_PAGO_ACCESS_TOKEN=TEST-XXXXXXXX
SES_SENDER_EMAIL=noreply@ducktickets.com

# Rate Limiting (moderate for testing)
RATE_LIMIT_CALLS=200
RATE_LIMIT_PERIOD=60

# Debug
DEBUG=false